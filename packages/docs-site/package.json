{
  "name": "docs-site",
  "version": "1.4.15",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx vue-demi-switch 3 && vite --host 0.0.0.0\" \"ts-node scripts/compile-docs.ts -- --watch\"",
    "build": "npm run compile-docs && npx vue-demi-switch 3 && vite build",
    "prerender": "prerender -c prerender.config.ts",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "compile-docs": "ts-node scripts/compile-docs.ts",
    "watch-compile-docs": "ts-node scripts/compile-docs.ts -- --watch",
    "deploy": "ts-node scripts/deploy.ts"
  },
  "dependencies": {
    "@he-tree/vue": "workspace:^2.9.3",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.6.2",
    "axios": "^1.5.1",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "dayjs": "^1.10.4",
    "floating-vue": "2.0.0-beta.24",
    "fs-extra": "^11.1.0",
    "fuse.js": "^6.6.2",
    "gitalk": "^1.7.2",
    "helper-js": "^3.1.2",
    "marked": "^4.0.18",
    "mdi-js": "^1.0.1",
    "scriptjs": "^2.5.9",
    "shikiji": "^0.6.10",
    "vue": "^3.3.4",
    "vue-demi": "latest",
    "vue-github-button": "^3.0.3",
    "vue-gtag": "^2.0.1",
    "vue-i18n": "^9.2.0",
    "vue-router": "^4.1.4"
  },
  "devDependencies": {
    "@types/scriptjs": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.4.5",
    "concurrently": "^8.2.1",
    "eslint": "^7.24.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "pug": "^3.0.2",
    "sass": "^1.32.8",
    "spa-prerender-cli": "^0.1.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-standard": "^21.0.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.2.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vite": "^4.4.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  }
}
